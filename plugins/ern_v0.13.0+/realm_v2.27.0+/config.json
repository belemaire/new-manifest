{
    "android": {
      "root": "android",
      "dependencies": [
        "org.nanohttpd:nanohttpd:2.2.0"
      ],
      "copy": [
          { "source": "android/src/main/jniLibs/**", "dest": "lib/src/main/jniLibs" }
      ]
    },
    "ios": {
      "copy": [
        { "source": "react-native/ios/**", "dest": "{{{projectName}}}/Libraries/RealmReact" }
      ],
      "pbxproj": {
         "addProject": [
          { "path": "RealmReact/RealmReact.xcodeproj", "group": "Libraries", "staticLibs": [ { "name": "libKCKeepAwake.a", "target": "KCKeepAwake" } ] }
        ],
        "addHeaderSearchPath": [
          "\"$(SRCROOT)/{{{projectName}}}/Libraries/RealmReact/**\""
        ]
      }
    }
  }
  
